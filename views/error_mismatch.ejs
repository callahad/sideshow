<% include _header %>
<h1><%= title %></h1>
<p><%- format(gettext("It looks like you were trying to sign in as %(claimed)s but were actually signed into Google as %(proven)s."),
       { claimed: '<b>' + escape(claimed) + '</b>', proven: '<b>' + proven + '</b>' }) %></p>
<p><%- format(gettext("Please sign into Google as %s then try again."),
       ['<b>' + escape(claimed) + '</b>']) %></p>
<p><a href="<%= personaUrl %>/sign_in#AUTH_RETURN_CANCEL">Try again</a></p>
<% include _footer %>
